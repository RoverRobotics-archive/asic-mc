{
  "ARMC6": {
    "common": [
      "-c",
      "--target=arm-arm-none-eabi",
      "-mthumb",
      "-g",
      "-O0",
      "-Wno-armcc-pragma-push-pop",
      "-Wno-armcc-pragma-anon-unions",
      "-Wno-reserved-user-defined-literal",
      "-Wno-deprecated-register",
      "-DMULADDC_CANNOT_USE_R7",
      "-fdata-sections",
      "-fno-exceptions",
      "-MMD",
      "-fshort-enums",
      "-fshort-wchar",
      "-DMBED_DEBUG",
      "-DMBED_TRAP_ERRORS_ENABLED=1",
      "--ide=mbed"
    ],
    "asm": [
      "--ide=mbed"
    ],
    "c": [
      "-D__ASSERT_MSG",
      "-std=gnu11"
    ],
    "cxx": [
      "-fno-rtti",
      "-fno-c++-static-destructors",
      "-std=gnu++14"
    ],
    "ld": [
      "--verbose",
      "--remove",
      "--show_full_path",
      "--legacyalign",
      "--any_contingency",
      "--keep=os_cb_sections",
      "--ide=mbed"
    ]
  }
}